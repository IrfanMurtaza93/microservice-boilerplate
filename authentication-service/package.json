    "babel-loader": "^8.0.0-beta.0",
    "bcrypt": "^5.0.0",
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^6.2.0",
    "ejs": "~2.5.7",
    "express": "~4.16.0",
    "express-joi-validator": "^2.0.1",
    "express-validation": "^1.0.2",
    "faker": "^5.1.0",
    "http-errors": "~1.6.2",
    "http-status-codes": "^1.3.0",
    "jade": "^1.11.0",
    "joi": "^14.3.1",
    "jsonwebtoken": "^8.4.0",
    "morgan": "~1.9.0",
    "mysql2": "^2.2.5",
    "nodemon": "^1.19.0",
    "pg": "^8.5.1",
