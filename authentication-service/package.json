{
  "name": "node-sequelize-boilerplate",
  "version": "1.0.0",
  "description": "This is boilerplate microservice",
  "scripts": {
    "start": "node ./server.js",
    "dev": "nodemon ./server.js",
    "db:create": "sequelize db:create --env development",
    "migrate": "sequelize db:migrate --env development",
    "migrate:undo": "sequelize db:migrate:undo --env development",
    "migration": "sequelize migration:generate --name",
    "test": "jest",
    "lint": "eslint . --max-warnings=0 --report-unused-disable-directives"
  },
  "author": "Irfan Murtaza",
  "license": "ISC",
  "dependencies": {
    "@babel/preset-env": "^7.4.5",
    "@babel/register": "^7.12.10",
